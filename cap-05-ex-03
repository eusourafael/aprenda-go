package main

import (
	"fmt"
)

const a = true
const b string = "Aprenda Go"
const c = 10.5
const d = 13

func main() {
	fmt.Printf("%v - %T\n", a, a)
	fmt.Printf("%v - %T\n", b, b)
	fmt.Printf("%v - %T\n", c, c)
	fmt.Printf("%v - %T\n", d, d)
}

